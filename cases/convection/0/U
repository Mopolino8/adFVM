FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 100 0 0 );

boundaryField
{
    patch0_half0
    {
        type            cyclic;
    }
    patch0_half1
    {
        type            cyclic;
    }
    patch1_half0
    {
        type            turbulentInletVelocity;
        Umean           uniform (100 0 0);
        r11   25;
        r22   25;
        r33   25;
        r12   0;
        r13   0;
        r23   0;
        lengthScale     0.04;
        timeScale       0.01;
    }
    patch1_half1
    {
        type            zeroGradient;
    }
    patch2_half0
    {
        type            cyclic;
    }
    patch2_half1
    {
        type            cyclic;
    }
}


