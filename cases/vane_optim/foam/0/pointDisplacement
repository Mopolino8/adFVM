FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 0 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    midplane1
    {
        type            cyclic;
    }
    midplane2
    {
        type            cyclic;
    }
    z1plane
    {
        type            cyclic;
    }
    z2plane
    {
        type            cyclic;
    }
    inlet
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    outlet
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    pressure
    {
        type            fixedValue;
        PRESSURE
    }
    suction
    {
        type            fixedValue;
        SUCTION
    }
}


